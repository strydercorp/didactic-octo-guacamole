{
  "manifest_version": 2,

  "name": "Full Story Watchermobile",
  "description": "Watches some fullstory",
  "version": "1.1",

  "permissions": [
    "tabs",
    "idle",
    "https://*.fullstory.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["script.js"]
  }
}
